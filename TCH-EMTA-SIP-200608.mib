-- CALLER ID DISPLAY TABLES

tchEmtaSipDeviceCallerNumberTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF TchEmtaSipDeviceCallerNumberEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "This table contains a list of preferred Calling Line Identity"
    ::=  { tchEmtaSipDeviceFeaturesCID 4 }

tchEmtaSipDeviceCallerNumberEntry    OBJECT-TYPE
    SYNTAX        TchEmtaSipDeviceCallerNumberEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "Each entry contains the preferred Calling Line Identity if
         its value is valid telephone number according to RFC2806."
    INDEX { tchEmtaSipCallerNumberRank }
    ::=  { tchEmtaSipDeviceCallerNumberTable 1 }

TchEmtaSipDeviceCallerNumberEntry  ::= SEQUENCE {
    tchEmtaSipCallerNumberRank  INTEGER,
    tchEmtaSipCallerNumberField INTEGER 
    }

tchEmtaSipCallerNumberRank  OBJECT-TYPE
    SYNTAX INTEGER (1..7)
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "Index for specifying the rank. For example, a value of 1
         means most preferred.
         If rank 1 not present or not valid, try using rank 2, etc."
    ::= { tchEmtaSipDeviceCallerNumberEntry 1 }

tchEmtaSipCallerNumberField  OBJECT-TYPE
    SYNTAX INTEGER {
        fromDiplayNameAnonymous(1),
        pAssertedIdUri(2),
        fromUri(3),
        pAssertedIdDisplayName(4),
        fromDisplayName(5),
        suppress(6),
        fromUriAnonymous(7)
        }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "The field from which callerId number will be taken and
         displayed on the phone.
         The number will be displayed ONLY if the string is a number.
         'fromDiplayNameAnonymous' means that when 'Anonymous' string is
         received in INVITE's From's Display Name field, the endpoint
         will only send 'Reason for Absence of Calling Line Identity
         parameter.
         'fromUriAnonymous' means that when 'Anonymous' string is
         received in INVITE's From's SIP URI field, the endpoint
         will only send 'Reason for Absence of Calling Line Identity
         parameter.
         Both anonoymous strings are case insensitive."
    ::= { tchEmtaSipDeviceCallerNumberEntry 2 }

tchEmtaSipDeviceCallerNameTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF TchEmtaSipDeviceCallerNameEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "This table contains a list of preferred Calling Party Name"
    ::=  { tchEmtaSipDeviceFeaturesCID 5 }

tchEmtaSipDeviceCallerNameEntry    OBJECT-TYPE
    SYNTAX        TchEmtaSipDeviceCallerNameEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "Each entry contains the preferred Calling Party Name"
    INDEX { tchEmtaSipCallerNameRank }
    ::=  { tchEmtaSipDeviceCallerNameTable 1 }

TchEmtaSipDeviceCallerNameEntry  ::= SEQUENCE {
    tchEmtaSipCallerNameRank  INTEGER,
    tchEmtaSipCallerNameField INTEGER
    }

tchEmtaSipCallerNameRank  OBJECT-TYPE
    SYNTAX INTEGER (1..5)
    MAX-ACCESS     not-accessible
    STATUS         current
    DESCRIPTION
        "Index for specifying the rank. For example, a value of 1
         means most preferred.  
         If rank 1 not present, try using rank 2, etc."
    ::= { tchEmtaSipDeviceCallerNameEntry 1 }

tchEmtaSipCallerNameField OBJECT-TYPE
    SYNTAX INTEGER {
         pAssertedIdDisplayName(1),
         fromDisplayName(2),
         pAssertedIdUri(3),
         fromUri(4),
         suppress(5)
         }
    MAX-ACCESS     read-only
    STATUS         current
    DESCRIPTION
        "The field from which calling party name will be taken and
         displayed on the phone.
         The name will be displayed ONLY if it is not suppressed(5)
         or not empty."
    ::= { tchEmtaSipDeviceCallerNameEntry 2 }

-- CALLERID SPECIAL NAMES

tchEmtaSipDeviceFeaturesCIDAbsenceReason OBJECT-TYPE
    SYNTAX BITS { 
        p(0), 
        c(1),
        s(2),
        o(3)
    } 
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
        "When particular string is received by endpoint in INVITE's From's
         DISPLAY NAME field, the endpoint sends special character to the
         phone in callerID type 'Reason for Absence of Calling Line
         Identity parameter' (type 4):
           'Anonymous' = P
           'Coin line/payphone' = C
           'Interaction with other service' = S
           'Unavailable' = O
         Each reason for absence can be controled independently by its bit.
         Bit set means the callerID will send the reason for absence.
         Bit unset means empty string will be sent in the callerID name field.
         By default only Anonymous is enabled."
    DEFVAL { { p } } 
    ::= { tchEmtaSipDeviceFeaturesCID 6 }

