        TruthValue
                FROM SNMPv2-TC ;

tchEmtaSip   MODULE-IDENTITY  
     LAST-UPDATED    "202006080000Z"
     ORGANIZATION    "Technicolor, Inc."
     CONTACT-INFO    "http://www.technicolor.com"
        DESCRIPTION
            "Controls the behavior of SIP Embedded MTA fuctions.
             Contains extra parameters on top of tchMta tree
             needed for SIP device to function properly.
             This MIB is active only at the MTA IP address.
             Can be part of MTA configuration file." 
             

        -- History

	REVISION 	"202006080000Z"
	DESCRIPTION "Adding new MIB : tchEmtaSipDeviceAuthHeader "

	REVISION 	"201708030000Z"
	DESCRIPTION "Adding new MIB : tchEmtaSipDeviceRegisterOnOffhook "
	
	REVISION     "201609130000Z"
	DESCRIPTION
         "Initial version.  Converted from SA/Cisco mib definition."

        ::= { tchMta 5 }

