-- CALL WAITING

tchEmtaSipDeviceFeaturesCallWaiting OBJECT IDENTIFIER ::= { tchEmtaSipDeviceFeatures 2 }

tchEmtaSipDeviceFeaturesCallWaitingOnDigitMap OBJECT-TYPE 
    SYNTAX SnmpAdminString 
    MAX-ACCESS read-only 
    STATUS current 
    DESCRIPTION 
        "Defines digit map to activate call waiting by user. 
        If not specified, user cannot control this feature and 
        tchEmtaSipFeaturesCallWaitingOffDigitMap is erased." 
    DEFVAL { "" } 
    ::= { tchEmtaSipDeviceFeaturesCallWaiting 3 } 

tchEmtaSipDeviceFeaturesCallWaitingOffDigitMap OBJECT-TYPE 
    SYNTAX SnmpAdminString 
    MAX-ACCESS read-only 
    STATUS current 
    DESCRIPTION 
        "Defines digit map to deactivate call waiting by user. 
        If not specified, user cannot control this feature and 
        tchEmtaSipFeaturesCallWaitingOnDigitMap is erased." 
    DEFVAL { "" } 
    ::= { tchEmtaSipDeviceFeaturesCallWaiting 4 }

tchEmtaSipDeviceFeaturesCallWaitingOnHoldTone OBJECT-TYPE 
    SYNTAX BITS {
           passive(0),
           active(1)
    }
    MAX-ACCESS read-only 
    STATUS current 
    DESCRIPTION 
        "Controls whether to play the tone while an endpoint is on hold.
         The tone is defined as callWaiting2 in tComLabs MIB.
         For a scenario: A-B talk, C calls A, A hook-flash to C
         - passive: tone is played on B
         - active: tone is played on A (during conversation with C)" 
    DEFVAL { { } } 
    ::= { tchEmtaSipDeviceFeaturesCallWaiting 5 }
    
tchEmtaSipDeviceFeaturesCallWaitingDuration OBJECT-TYPE 
    SYNTAX Unsigned32
    UNITS "seconds"
    MAX-ACCESS read-only
    STATUS current 
    DESCRIPTION 
        "Controls duration of call waiting tone before sending busy-here.? "
    DEFVAL { 180 } 
    ::= { tchEmtaSipDeviceFeaturesCallWaiting 6 }

