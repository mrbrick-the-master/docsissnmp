    tchRgQosSwitchClassPrioritySetPort BITS,
    tchRgQosSwitchClassPrioritySetDa   INTEGER,
    tchRgQosSwitchClassPrioritySetVlan BITS,
    tchRgQosSwitchClassPrioritySetTos  BITS
    }

tchRgQosSwitchClassPriorityEnable OBJECT-TYPE
    SYNTAX BITS {
        port(0),
        dst-mac(1),
        vlan(2),
        tos(3)
    }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "Defines which classifiers to use for determinig egress frame priority.
             If more than one is selected, the order for classification is determined
             by the other MIB objects.
             port(0): priority is determined by the ingress port
             dst-mac(1): priority is determined by destination MAC address - not implemented
             vlan(2): priority is determined by IEEE 802.1p
             tos(3): priorty is determined by IP ToS (IPv4) or IP TC (IPv6)"
    -- DEFVAL { '20'h }
    DEFVAL { { vlan } }
    ::= { tchRgQosSwitchClassPriorityEntry 1 }

tchRgQosSwitchClassPriorityPort OBJECT-TYPE
    SYNTAX      INTEGER (0..3)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Defines priority of port classifier lookup.
             Priority 0 is the highest."
    DEFVAL { 2 }
    ::= { tchRgQosSwitchClassPriorityEntry 2 }

tchRgQosSwitchClassPriorityDa OBJECT-TYPE
    SYNTAX      INTEGER (0..3)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "Defines priority of DST MAC classifier lookup.
             Priority 0 is the highest.
             This is not implemented."
    DEFVAL { 3 }
    ::= { tchRgQosSwitchClassPriorityEntry 3 }

tchRgQosSwitchClassPriorityVlan OBJECT-TYPE
    SYNTAX      INTEGER (0..3)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Defines priority of 802.1p classifier lookup.
             Priority 0 is the highest."
    DEFVAL { 0 }
    ::= { tchRgQosSwitchClassPriorityEntry 4 }

tchRgQosSwitchClassPriorityTos OBJECT-TYPE
    SYNTAX      INTEGER (0..3)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Defines priority of ToS/TC classifier lookup.
             Priority 0 is the highest."
    DEFVAL { 1 }
    ::= { tchRgQosSwitchClassPriorityEntry 5 }

tchRgQosSwitchClassPrioritySetPort OBJECT-TYPE
    SYNTAX BITS {
        port-0-h(0),
        port-0-l(1),
        port-1-h(2),
        port-1-l(3),
        port-2-h(4),
        port-2-l(5),
        port-3-h(6),
        port-3-l(7),
        port-4-h(8),
        port-4-l(9)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This classifier sets the frame priority for defined ingress port.
             -h is the high significant bit, -l is the low significant bit."
    DEFVAL { { } }
    ::= { tchRgQosSwitchClassPriorityEntry 6 }

tchRgQosSwitchClassPrioritySetDa OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "This is not implemented."
    DEFVAL { 0 }
    ::= { tchRgQosSwitchClassPriorityEntry 7 }

tchRgQosSwitchClassPrioritySetVlan OBJECT-TYPE
    SYNTAX      TchRgQosVlanToPriority
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This classifier sets the frame priority for each 802.1p priority."
    -- DEFVAL { 'fa50'h }
    DEFVAL { { vlan-7-h, vlan-7-l, vlan-6-h, vlan-6-l, vlan-5-h, vlan-4-h, vlan-3-l, vlan-2-l } }
    ::= { tchRgQosSwitchClassPriorityEntry 8 }

tchRgQosSwitchClassPrioritySetTos OBJECT-TYPE
    SYNTAX      TchRgQosTosToPriority
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This classifier sets the frame priority for each ToS or TC.
            The default value is 0xffffffffaaaaaaaa5555555500000000."
    -- DEFVAL { 'ffffffffaaaaaaaa5555555500000000'h }
    ::= { tchRgQosSwitchClassPriorityEntry 9 }

tchRgQosEnable OBJECT-TYPE
    SYNTAX INTEGER {
        disable(0),
        enable(1)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This will enable QoS classification for use with tchRgQosDestIp."
    DEFVAL { 0 }
    ::= { tchRgQosLan 3 }

tchRgQosDestIp OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "This will set the IP address that has higher QoS priority."
    ::= { tchRgQosLan 4 }

