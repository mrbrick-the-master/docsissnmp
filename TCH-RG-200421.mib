--=========== FIRST INDEX SELECTING WHICH LOG =======================

-- Index to specify which log is addressed: eRouter, or one per SSID
tchRgEventLogTypeIndex OBJECT-TYPE
    SYNTAX INTEGER {
            eRouter(1), 
            wifi(2)
     }
