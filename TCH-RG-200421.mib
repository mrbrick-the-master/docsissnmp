    ::= { tchRgDot11ApsScanResultsTable 1  }

TchRgDot11ApsScanResultsEntry ::=
    SEQUENCE    {
        tchRgDot11ApsScanValid           INTEGER,
        tchRgDot11ApsScanNetworkName     OCTET STRING,
        tchRgDot11ApsScanSecurityMode    OCTET STRING,
        tchRgDot11ApsScanPhyMode         OCTET STRING,
        tchRgDot11ApsScanRssi            INTEGER,
        tchRgDot11ApsScanChannel         INTEGER,
        tchRgDot11ApsScanMacAddress      PhysAddress
    }

