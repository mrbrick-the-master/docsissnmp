    ::= { tchRgIperfSpeedTest 1 }

tchRgIperfServerAddr OBJECT-TYPE
    SYNTAX      InetAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "IP Address, v4 or v6, of Iperf Server on the WAN.
             IPv6 is not supported currently."

    ::= { tchRgIperfSpeedTest 2 }

tchRgIperfProtocol OBJECT-TYPE
    SYNTAX  INTEGER {
        udp(1),
        tcp(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "IP protocol to use during Iperf test.
             Default value is TCP."
    DEFVAL { 2 }
    ::= { tchRgIperfSpeedTest 3 }

tchRgIperfMode OBJECT-TYPE
    SYNTAX  INTEGER {
        ipv4(1),
        ipv6(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Whether the Iperf Test will be conducted in ipv4 or ipv6.
             IPv6 not supported currently"
    DEFVAL { 1 }
    ::= { tchRgIperfSpeedTest 4 }

tchRgIperfPort OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The UDP/TCP port to be used for the Iperf Test."
    DEFVAL { 5001 }
    ::= { tchRgIperfSpeedTest 5 }

tchRgIperfTestTime OBJECT-TYPE
    SYNTAX  INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Duration of Iperf Test, in secs"
    DEFVAL { 30 }
    ::= { tchRgIperfSpeedTest 6 }

tchRgIperfBandwidth OBJECT-TYPE
    SYNTAX  INTEGER {
        kiloBitsPerSec(1),
        megaBitsPerSec(2),
        gigaBitsPerSec(3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The format of the iperf test results.  The options are KBits/sec, Mbits/sec, or GBits/sec.
             Only megaBitsPerSec is currently supported."
    DEFVAL { 2 }
    ::= { tchRgIperfSpeedTest 7 }

tchRgIperfWindowSize OBJECT-TYPE
    SYNTAX  INTEGER 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The value window size, is only valid if the test protocol is TCP. The maximum value of a TCP window size is 65KB. 
             The default for this iperf test is 64KB.  WindowSize should be updated in 1kB increments (1000)."
    DEFVAL { 64000 }
    ::= { tchRgIperfSpeedTest 8 }

tchRgIperfMSS OBJECT-TYPE
    SYNTAX  INTEGER 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "The value mss, or maximum segment size, is only valid if the test protocol is TCP. MSS is usually MTU-40bytes. Therefore the default value is 1460, 1500(MTU size) - 40. 
             The default for this iperf test is 64KB"
    DEFVAL { 1460 }
    ::= { tchRgIperfSpeedTest 9 }

tchRgIperfParallelStreams OBJECT-TYPE
    SYNTAX  INTEGER 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Parallel Streams defines the number of iperf test to be run at the same time.  The default value is 1.  Parallel Streams is only valid for TCP"
    DEFVAL { 1 }
    ::= { tchRgIperfSpeedTest 10 }

tchRgIperfTestStatus OBJECT-TYPE
    SYNTAX  INTEGER {
        none(0),
        requested(1),
        requestedUpstream(2),
        requestedDownstream(3),
        complete(4),
        cancel(5),
        error(6)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
            "Iperf Test Status. Requested, RequestedUpstreami, RequestedDownStream, and Cancel are the only writable values.
             None means no test is running.
             Requested means Upstream and Downstream test is running.
             RequestedUpstream means only upstream test is running.
             RequestedDownstream means only downstream test is running.
             Complete means all test have been executed. 
             Cancel means to cancel any executing test.
             Error means there was a problem executing the test. "
    ::= { tchRgIperfSpeedTest 11 }

tchRgIperfDownstreamTestResults OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Downstream Results as specified when setting tchRgIperfBandwidth. 
         Not valid when Requested Test is Upstream only"
    ::= { tchRgIperfSpeedTest 12 }

tchRgIperfUpstreamTestResults OBJECT-TYPE
    SYNTAX      DisplayString
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Upstream Results as specified when setting tchRgIperfBandwidth.
         Not valid when Requested Test is Downstream only"
    ::= { tchRgIperfSpeedTest 13 }

tchRgIperfEnableWebuiPage OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
        "Sets webui bitmask to enable Iperf Speed Test Page in GUI."
    ::= { tchRgIperfSpeedTest 14 }

