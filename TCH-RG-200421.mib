    ::= { tchRgDot11AccessMacEntry 4 }

-- PRIMARY BSS SPECIFIC

tchRgDot11BssPrimary OBJECT IDENTIFIER ::= { tchRgDot11Bss 3 }

tchRgDot11BssPrimarySsidType OBJECT-TYPE
    SYNTAX INTEGER {
        mac6char(1),
        mac6char-prefix(2),
        mac4char-prefix(3),
        prefix-force(4),
        prefix-default(5)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This object specifies the type of SSID used.
         mac6char(1): If tchRgDot11BssSsid.32 is empty, the SSID is the last
                      3 bytes of DOCSIS MAC address, displayed as 6 lowercase
                      hex characters.
                      If tchRgDot11BssSsid.32 is not empty, SSID is the value from
                      tchRgDot11BssSsid.32 (set by SNMP or the user).
         mac6char-prefix(2): the same as mac6char(2) with the prefix addition
                             defined in tchRgDot11BssPrimarySsidPrefix.
                             If  tchRgDot11BssSsid.32 is not empty, this prefix
                             does not show.
         mac4char-prefix(3): the same as mac6char-prefix(2) but 4 MAC characters
                             instead of 6.
         prefix-force(4): The prefix is always included, even if tchRgDot11BssSsid.32
                          is empty.
		 prefix-default(5): The prefix defined in tchRgDot11BssPrimarySsidPrefix replace the current default SSID prefix
         Note: stored in non-vol. mac6char(1) after factory reset"
    ::= { tchRgDot11BssPrimary 1 }

tchRgDot11BssPrimarySsidPrefix OBJECT-TYPE
    SYNTAX SnmpAdminString (SIZE(1..16))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The prefix for primary SSID, if tchRgDot11BssSsid.32 is empty
         and tchRgDot11BssPrimarySsidType is set to mac6char-prefix(2).
         Stored in non-vol. Empty after factory reset."
    ::= { tchRgDot11BssPrimary 2 }

tchRgDot11BssPrimarySsidSuffix OBJECT-TYPE
    SYNTAX INTEGER {
        disable(0),
        suffix2-4(1),
        suffix5(2),
        suffixboth(3)
        }
    MAX-ACCESS read-write
    STATUS     current
    DESCRIPTION
        "The suffix for primary SSID. Default no suffix is added. 
        suffix2-4(1): -2.4G will be added as suffix for the 2.4Ghz band
        suffix5(2): -5G will be added as suffix for the 5Ghz band
        suffixboth(3): Both -2.4G and -5G will be added as suffix to the corresponding frequency band.
         "  
    DEFVAL { 0 }
    ::= { tchRgDot11BssPrimary 3 }

