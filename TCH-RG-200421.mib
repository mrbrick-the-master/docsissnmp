---------------------------------------
-- Dot11nExt - Radio Settings Table
---  Table to support Multiple 802.11n Radios
---  Note: Indexes may be set to support specific hardware interfaces.
---  Index 32 MUST match tchRgDot11n.
---------------------------------------

tchRgDot11nExt OBJECT IDENTIFIER ::= { tchRgDot11 7 }

tchRgDot11nExtTable  OBJECT-TYPE
    SYNTAX      SEQUENCE OF TchRgDot11nExtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Multiple 802.11n Radio Configuration Table
                 Index 32 should be set for Radio 0 -- 2.4 GHz if populated.
                 Index 112 should be set for Radio 1"
    ::= { tchRgDot11nExt 1 }

tchRgDot11nExtEntry OBJECT-TYPE
    SYNTAX      TchRgDot11nExtEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A row in the table which represents parameters for an 802.11n Radio."
    INDEX   { ifIndex }
    ::= { tchRgDot11nExtTable 1 }

TchRgDot11nExtEntry ::=
    SEQUENCE {
                tchRgdot11nExtMode               INTEGER,
                tchRgdot11nExtPhyRate            INTEGER,
                tchRgdot11nExtBand               INTEGER,
                tchRgdot11nExtBandWidth          INTEGER,
                tchRgdot11nExtSideBand           INTEGER,
                tchRgdot11nExtObssCoex           INTEGER,
                tchRgdot11nExtMulticastRate      INTEGER,
                tchRgdot11nExtBSenable	         INTEGER,
                tchRgdot11nExtKeepRadioAlive     INTEGER,
                tchRgdot11nExtUNII2ExtChannelEnable     INTEGER
        }

tchRgdot11nExtMode OBJECT-TYPE
    SYNTAX INTEGER {
        auto(1),
        off(2),
        n-only(3),
        b-mode(16),
        g-mode(32),
        bg-mode(48),
        a-mode(64),
        n-only-bitmask(128),    -- to differentiate from n-only(3)
        gn-mode(160),
        bgn-mode(176),
        an-mode(192),
        ac-only(256),
        nac-only(384),
        anac-mode(448)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Determines which 802.11 mode the radio will run in.
         This object must be set by either the legacy or bitmask method.

         In legacy method,
         auto(1)  : supports all the 802.11 modes available in the radio.
         off(2)   : supports the legacy mode only (11b/g in 2.4GHz and 11a in 5GHz).
         n-only(3): accepts 11n-capable clients only but supports all the modes available in the radio.

         In bitmask method,
         b-mode(16)    : supports 11b mode in 2.4GHz.
         g-mode(32)    : supports 11g mode in 2.4GHz.
         bg-mode(48)   : supports 11b/g mode in 2.4GHz (16+32).
         a-mode(64)    : supports 11a mode in 5GHz.
         n-only-bitmask(128)   : accepts 11n-capable clients only but supports both legacy and 11n modes in 2.4GHz or 5GHz.
         gn-mode(160)  : supports 11g/n mode in 2.4GHz (32+128).
         bgn-mode(176) : supports all 11b/g/n modes in 2.4GHz (16+32+128).
         an-mode(192)  : supports 11a/n modes in 5GHz (64+128).
         ac-only(256)  : accepts 11ac-capable clients only but supports 11a/n/ac modes in 5GHz.
         nac-only(384) : accepts 11n-capable clients only but supports 11a/n/ac modes in 5GHz (128+256).
         anac-mode(448): supports all 11a/n/ac modes in 5GHz (64+128+256).

         Note: Some options may not be available depending on the radio model.
         Note: The change of this object value will also affect other MIB selections, including tchRgdot11nExtBandWidth.
         This object is stored in non-vol and defaults to auto(1) after factory reset."
     ::= { tchRgDot11nExtEntry 1 }

tchRgdot11nExtPhyRate OBJECT-TYPE
    SYNTAX INTEGER {
        auto(0),
        legacy(1),
        mbits6-5or13-5(2),
        mbits13or27(3),
        mbits19-5or40-5(4),
        mbits26or54(5),
        mbits39or81(6),
        mbits52or108(7),
        mbits58-5or121-5(8),
        mbits65or135(9),
        reserved1(10),
        reserved2(11),
        reserved3(12),
        reserved4(13),
        mbits78or162(14),
        mbits104or216(15),
        mbits117or243(16),
        mbits130or270(17)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Forces the N Phy transmission rate for the AP to a particular speed.  Each pair of rates
         corresponds to a 20 MHz or 40 MHz channel configuration.
         This object is stored in non-vol and defaults to auto(0) after factory reset.

         This MIB is being degraded, since it is better for higher band width devices to run in 'auto' mode"
    DEFVAL { auto }
    ::= { tchRgDot11nExtEntry 2 }


tchRgdot11nExtBand OBJECT-TYPE
    SYNTAX INTEGER {
        band-2-4G(1),
        band-5G(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Determines which Band that we will be operating in. Either the 2.4G or the 5G Hz.
         Note: This is for N cards only.
         This object is stored in non-vol and defaults to band-2-4G(1) after factory reset."
    ::= { tchRgDot11nExtEntry 3 }

tchRgdot11nExtBandWidth OBJECT-TYPE
    SYNTAX INTEGER {
        width-20MHz(1),
        width-40MHz(2),
        width-80MHz(3)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Determines which Band Width that we will be operating in. Either the 20MHz or the 40MHz.
         Note: This is for N cards only, and this also affect other MIB selections such as tchRgdot11nSideBand.
         The 80 MHz channel width is only applicable for 802.11ac capable cards. If a user sets this to 80 MHz, SNMPset would fail for non-802.11ac cards
         This object is stored in non-vol and defaults to ? after factory reset."
    ::= { tchRgDot11nExtEntry 4 }

tchRgdot11nExtSideBand OBJECT-TYPE
    SYNTAX INTEGER {
        upper(1),
        lower(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Determines which side Band the channel will operate in.
         Note: This is for N cards only, this mib is dependent of what tchRgdot11nBandWidth is selected and only
         applies when 40MHz is selected.
         This object is stored in non-vol and defaults to lower(2) after factory reset."
    ::= { tchRgDot11nExtEntry 5 }

tchRgdot11nExtObssCoex OBJECT-TYPE
        SYNTAX INTEGER {
                disable(0),
                enable(1)
                }
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
                "Allows MSO to control OBSS (Overlapping BSS) Coexistance parameter for 802.11n 40MHz channel width.
                 By default it is enabled and is stored in non-vol
                 Factory reset would clear it and would be enable(1) after factory reset.
                 When disabled, the 802.11n Wifi would stay in 40 MHz channel-width inspite of interference from other
                 wifi channels. While enabled, 802.11n would coexist with 802.11g channels using 20 MHz default channel-width
                 "
        ::= { tchRgDot11nExtEntry 6 }

tchRgdot11nExtMulticastRate OBJECT-TYPE
    SYNTAX INTEGER {
        disable(0),
        legacy(1),
        mbits6-5or13-5(2),
        mbits13or27(3),
        mbits19-5or40-5(4),
        mbits26or54(5),
        mbits39or81(6),
        mbits52or108(7),
        mbits58-5or121-5(8),
        mbits65or135(9),
        reserved1(10),
        reserved2(11),
        reserved3(12),
        reserved4(13),
        mbits78or162(14),
        mbits104or216(15),
        mbits117or243(16),
        mbits130or270(17)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Forces the dot11n Multicast Phy transmission rate.  Legacy mode forces the rate to be set
                 by dot11MulticastRate.
         This object is stored in non-vol and defaults to disable(0) after factory reset."
    DEFVAL { 0 }
    ::= { tchRgDot11nExtEntry 7 }

tchRgdot11nExtBSenable OBJECT-TYPE
    SYNTAX INTEGER {
        disable(0),
        enable(1)
        }
        MAX-ACCESS read-write
        STATUS     current
        DESCRIPTION
                "Controls and reflects the state of Wifi Radio's Band steering capability for all cards.  
				If set to enable(1), then Band steering will be enabled.
				Set to disable(0) to disable the Band steering feature."
		DEFVAL { 0 }
        ::= { tchRgDot11nExtEntry 8 }

tchRgdot11nExtKeepRadioAlive OBJECT-TYPE
    SYNTAX INTEGER {
        disable(0),
        enable(1)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "When it is set to enable(1) and wireless button is pressed, Radio will be turned OFF irrespective of Hotspot
        SSIDs Enable state. When it is set to disable(0), Radio will be turned OFF only if HotSpot SSIDs Enable state 
        is disabled. Default value is enable(1) and is stored in non-vol. Factory reset would clear it and would be 
        enable(1) after factory reset."
    DEFVAL { 1 }
    ::= { tchRgDot11nExtEntry 9 }

tchRgdot11nExtUNII2ExtChannelEnable OBJECT-TYPE
    SYNTAX INTEGER {
        disable(0),
        enable(1)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "When it is set to disable(0) UNII2-Ext channels are removed from Auto channel selection.
         When it is set to enable(1) UNII2-Ext channels are included in Auto channel selection.
         Default value is disable(0) and is stored in non-vol. Factory reset would clear it and would be 
         disable(0) after factory reset.
         
         UNII2-Ext channels are 100, 104, 108, 112, 116, 120, 124, 128, 132, 136, 140, 144
         This is applicable for 5GHz radio only.        
         "
    DEFVAL { 0 }
    ::= { tchRgDot11nExtEntry 10 }

