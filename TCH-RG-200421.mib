tchRgIGMP OBJECT IDENTIFIER ::= { tchRg 15 }

tchRgIGMPMaxRspTime OBJECT-TYPE
    SYNTAX INTEGER {
        igmpV2(1),
        igmpV3(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "There is a difference in IGMPv2 and IGMPv3 to translate the MAX RSP CODE to MAX RSP TIME.
                 In IGMPv2 MAX RSP TIME = MAX RSP CODE/10.
                 In IGMPv3, When Max RSP Code > 128, max response time is calculated differently.
                 When this MIB is set to IGMPv2(1) modem will set max response time according to IGMPv2 standard.
                 This setting is recommended for older CMTS implementations and when random response time of 0 to 10 sec is desired.
                 When this MIB is set to IGMPv3(2) modem will set max response time according to IGMPv3 standard.
                 This setting is recommended for new CMTS implementations and when random response time of 0 to above 10 seconds is desired.
         This object is stored in non-vol and defaults to IGMPv2(1) after factory reset."
        DEFVAL { 1 }
    ::= { tchRgIGMP 1 }

tchRgIGMPTransferFeature OBJECT-TYPE
    SYNTAX INTEGER {
        disable(0),
        enable(1)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "0: Disable, IGMP transfer feature (IGMPv2 transfer to IGMPv3) is disable.
		 1: Enable, IGMP transfer feature (IGMPv2 transfer to IGMPv3) is enable."
        DEFVAL { 0 }
    ::= { tchRgIGMP 2 }

tchRgIGMPVersion OBJECT-TYPE
    SYNTAX  INTEGER {
        igmpv2Igmpv3(0),
        igmpv3Only(1),
        igmpv2Only(2)
        }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "0: IGMPv2 and IGMPv3.
         1: IGMPv3 only.
         2: IGMPv2 only."
    DEFVAL { igmpv2Igmpv3 }
    ::= { tchRgIGMP 3 }

tchRgIGMPEnable OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable/Disable IGMP feature "
    DEFVAL { true }
    ::= { tchRgIGMP 4 }

tchRgIGMPMaxSessions OBJECT-TYPE
    SYNTAX      Unsigned32 (1..50)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
         "This MIB will restrict the maximum number of concurrent IGMP Multicast sessions for a unique Multicast group.
	 The minimum value is 1 session and the maximum value is 50 sessions, the factory default value is 30 sessions."
    DEFVAL { 30}
    ::= { tchRgIGMP 5 }

